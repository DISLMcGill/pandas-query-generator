{
  "$schema": "http://json-schema.org/schema#",
  "type": "object",
  "properties": {
    "entities": {
      "type": "object",
      "properties": {
        "customer": {
          "type": "object",
          "properties": {
            "primary_key": { "type": "string" },
            "properties": {
              "type": "object",
              "properties": {
                "C_CUSTKEY": {
                  "type": "object",
                  "properties": {
                    "type": { "type": "string" },
                    "min": { "type": "integer" },
                    "max": { "type": "integer" }
                  },
                  "required": ["max", "min", "type"]
                },
                "C_NAME": {
                  "type": "object",
                  "properties": {
                    "type": { "type": "string" },
                    "starting_character": {
                      "type": "array",
                      "items": { "type": "string" }
                    }
                  },
                  "required": ["starting_character", "type"]
                },
                "C_STATUS": {
                  "type": "object",
                  "properties": {
                    "type": { "type": "string" },
                    "values": { "type": "array", "items": { "type": "string" } }
                  },
                  "required": ["type", "values"]
                }
              },
              "required": ["C_CUSTKEY", "C_NAME", "C_STATUS"]
            },
            "foreign_keys": { "type": "object" }
          },
          "required": ["foreign_keys", "primary_key", "properties"]
        },
        "order": {
          "type": "object",
          "properties": {
            "primary_key": { "type": "string" },
            "properties": {
              "type": "object",
              "properties": {
                "O_ORDERKEY": {
                  "type": "object",
                  "properties": {
                    "type": { "type": "string" },
                    "min": { "type": "integer" },
                    "max": { "type": "integer" }
                  },
                  "required": ["max", "min", "type"]
                },
                "O_CUSTKEY": {
                  "type": "object",
                  "properties": {
                    "type": { "type": "string" },
                    "min": { "type": "integer" },
                    "max": { "type": "integer" }
                  },
                  "required": ["max", "min", "type"]
                },
                "O_TOTALPRICE": {
                  "type": "object",
                  "properties": {
                    "type": { "type": "string" },
                    "min": { "type": "number" },
                    "max": { "type": "number" }
                  },
                  "required": ["max", "min", "type"]
                },
                "O_ORDERSTATUS": {
                  "type": "object",
                  "properties": {
                    "type": { "type": "string" },
                    "values": { "type": "array", "items": { "type": "string" } }
                  },
                  "required": ["type", "values"]
                }
              },
              "required": [
                "O_CUSTKEY",
                "O_ORDERKEY",
                "O_ORDERSTATUS",
                "O_TOTALPRICE"
              ]
            },
            "foreign_keys": {
              "type": "object",
              "properties": {
                "O_CUSTKEY": { "type": "array", "items": { "type": "string" } }
              },
              "required": ["O_CUSTKEY"]
            }
          },
          "required": ["foreign_keys", "primary_key", "properties"]
        }
      },
      "required": ["customer", "order"]
    }
  },
  "required": ["entities"]
}
